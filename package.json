      "git://github.com/stephenwf/manifesto.git#0bd82b210d0247537c30a1d78532c00e32fa5fa1",
    "manifold": "git://github.com/IIIF-Commons/manifold.git#v1.2.16",
    "material-components-web": "^0.30.0",
